[Unit]
Description=<desc>
After=sshd.service
After=network-online.target
Wants=sshd.service
Wants=network-online.target

[Service]
User=<user>
ExecStart=/usr/bin/autossh -M 0 -N -g -o "ServerAliveInterval 60" -o "ServerAliveCountMax 3" -R 0.0.0.0:<port>:127.0.0.1:22 <user>@<host>

[Install]
WantedBy=multi-user.target
